{
  "rewrites": [
    {
      "source": "/api/todos/:path*",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/chat/:path*",
      "destination": "/api/index.py"
    },
    {
      "source": "/docs",
      "destination": "/api/index.py"
    },
    {
      "source": "/openapi.json",
      "destination": "/api/index.py"
    }
  ]
}
